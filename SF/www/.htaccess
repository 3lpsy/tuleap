<IfModule mod_php3.c>
<Files projects>
ForceType application/x-httpd-php3
</Files>
<Files users>
ForceType application/x-httpd-php3
</Files>
<Files foundry>
ForceType application/x-httpd-php3
</Files>
</IfModule>

<IfModule mod_php4.c>
<Files projects>
ForceType application/x-httpd-php
</Files>
<Files users>
ForceType application/x-httpd-php
</Files>
<Files foundry>
ForceType application/x-httpd-php
</Files>
</IfModule>

<IfModule sapi_apache2.c>
<Files projects>
 AcceptPathInfo on
 SetOutputFilter PHP
 SetInputFilter PHP
</Files>
<Files users>
 AcceptPathInfo on
 SetOutputFilter PHP
 SetInputFilter PHP
</Files>
<Files foundry>
 AcceptPathInfo on
 SetOutputFilter PHP
 SetInputFilter PHP
</Files>
</IfModule>
