class ArtifactCondition {
    
    var $id;
    var $field;
    var $values;
    
    function ArtifactCondition($id, $field, $values) {
        $this->id        = $id;
        $this->setValues($values);
        $this->setField($field);
    }
    
    function getField() {
        return $this->field;
    }
    function setField($field) {
        $this->field = $field;
    }
    function getValues() {
        return $this->values;
    }
    function setValues($values) {
        $this->values = $values;
    }
}
