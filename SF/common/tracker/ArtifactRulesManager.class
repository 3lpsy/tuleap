class ArtifactRulesManager {

	/**
	 * The ArtifactType object.
	 *
	 * @var		object	$artifact_type.
	 */
	var $artifact_type; 

	/**
	 *	ArtifactRulesManager - constructor.
	 *
	 *	@param	object	The Group object.
	 *	@param	int		The id # assigned to this artifact type in the db.
	 */
	function ArtifactRulesManager(&$Group, $artifact_type_id) {
        
		$this->artifact_type =& new ArtifactType($Group, $artifact_type_id);
	}
}

