{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Enalean/mustache.php.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Enalean/Restler.git"
    }
  ],
  "require": {
    "paragonie/random_compat": "^2.0",
    "mustache/mustache": "dev-feature/strict-variables",
    "ezyang/htmlpurifier": "^4.9.3",
    "firebase/php-jwt": "^5.0.0",
    "paragonie/sodium_compat": "^1.6",
    "Luracast/Restler": "3.0.0-RC6",
    "php-amqplib/php-amqplib": "^2.7",
    "delight-im/cookie": "^3.1",
    "paragonie/easydb": "^1.3"
  },
  "config": {
    "platform": {
      "php": "5.6.25",
      "ext-bcmath": "0",
      "ext-mbstring": "0"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "^1.0"
  }
}
