<div class="sidebar-nav">
    <div class="project-title-container" data-content="{{{ project_privacy }}}">
        <a href="{{ projectLink }}" class="project-title">
            {{#projectIsPublic}}
                <i class="icon-unlock-alt"></i>
            {{/projectIsPublic}}
            {{^projectIsPublic}}
                <i class="icon-lock"></i>
            {{/projectIsPublic}}

            {{{ projectName }}}
        </a>
    </div>
    <ul class="nav nav-list">
        {{#sidebar}}
            <li {{#enabled}}class="active"{{/enabled}}>
                <a href="{{{ link }}}" title="{{{ name }}}"><i class="{{ icon }}"></i> {{{ label }}}</a>
            </li>
        {{/sidebar}}
    </ul>
    {{# is_sidebar_collapsable }}
        <i class="icon-chevron-left sidebar-collapse"></i>
    {{/ is_sidebar_collapsable }}
    <div class="sidebar-about">
        <p>{{{ powered_by }}}</p>
        <p>{{{ copyright }}}</p>
    </div>
</div>
