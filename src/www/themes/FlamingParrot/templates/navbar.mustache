<div class="navbar navbar-fixed-top">
    {{# has_motd }}<div class="motd">{{{ motd }}}</div>{{/ has_motd }}
    <a id="navbar-logo" href="/"><div class="logo"></div></a>
    <div class="navbar-inner">
        <div class="container-fluid">
            <ul class="nav">
                {{# navbar_items }}
                    {{# is_projects }}
                        {{> navbar-projects }}
                    {{/ is_projects }}
                    {{# is_link }}
                        {{> navbar-item-link }}
                    {{/ is_link }}
                    {{# is_dropdown }}
                        {{> navbar-item-dropdown }}
                    {{/ is_dropdown }}
                {{/ navbar_items }}
            </ul>
            {{> navbar-user-navigation }}
        </div>
    </div>
</div>
