<div class="alert alert-info">
    <span class=" lab_features">This is a lab feature that is subjected to
    change in a near future. Please get in touch with us to
    <a href="mailto:tuleap-devel@tuleap.net">provide any feedback</a> about
    this work in progress, thanks!</span>
</div>
{{#can_add_backlog_item}}
<p>
    <button class="btn">{{add_new_backlog_item}}</button>
</p>
{{/can_add_backlog_item}}
<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>{{title}}</th>
            <th>{{points}}</th>
            <th>{{parent}}</th>
        </tr>
    </thead>
    <tbody class="milestone-content-rows">
        {{#rows}}
        <tr data-artifact-id="{{id}}">
            <td><a href="{{url}}">{{id}}</a></td>
            <td>{{title}}</td>
            <td>{{points}}</td>
            <td><a href="{{parent_url}}">{{parent_title}}</a></td>
        </tr>
        {{/rows}}
    </tbody>
</table>

