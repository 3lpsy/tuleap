<h3>All closed {{backlog_item_type}}</h3>

{{#has_something_done}}
<p>Please find below the items that are closed. You cannot change the
   priorities.</p>
<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>{{title}}</th>
            <th>{{parent}}</th>
        </tr>
    </thead>
    <tbody>
        {{#done_collection}}
        <tr data-artifact-id="{{id}}">
            <td><a href="{{url}}">{{id}}</a></td>
            <td>{{title}}</td>
            <td><a href="{{parent_url}}">{{parent_title}}</a></td>
        </tr>
        {{/done_collection}}
    </tbody>
</table>
{{/has_something_done}}
{{^has_something_done}}
<p>Nothing done yet</p>
{{/has_something_done}}
