{{#has_something}}
    {{#has_something_done}}
        <h3 class="inconsistent-items-title">{{inconsistent_items_title}}</h3>
        <p>{{inconsistent_items_intro}}</p>
        <div class="row-fluid">
        <table class="table table-striped table-hover table-bordered span8">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>{{type}}</th>
                    <th>{{title}}</th>
                    <th>{{parent}}</th>
                </tr>
            </thead>
            <tbody>
                {{#done_collection}}
                    <tr data-artifact-id="{{id}}">
                        <td><a href="{{url}}">{{id}}</a></td>
                        <td><span class="label">Type</span></td>
                        <td>{{title}}</td>
                        <td><a href="{{parent_url}}">{{parent_title}}</a></td>
                    </tr>
                {{/done_collection}}
            </tbody>
        </table>
        </div>
    {{/has_something_done}}
{{/has_something}}
