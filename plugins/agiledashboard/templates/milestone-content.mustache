{{#backlog_collection}}
<tr class="submilestone-element milestone-open" data-artifact-id="{{id}}">
    <td><a href="{{url}}">{{id}}</a></td>
    <td><span class="{{ color }} xref-in-title label">{{type}}</span></td>
    <td class="milestone-element-title">{{title}}</td>
    <td class="submilestone-element-remaining-effort">{{points}}</td>
    <td>{{status}}</td>
</tr>
{{/backlog_collection}}
