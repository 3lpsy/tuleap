	:am:bs:mi:ul:\
	:co#80:li#24:\
	:al=\EE:bl=^G:cd=\EY:ce=\ET:cl=\E*:cm=\E=%+ %+ :cr=^M:\
	:dc=\EW:dl=\ER:do=^J:ei=\Er:ho=^^:im=\Eq:le=^H:mh=\EG2:nd=^L:\
	:sf=^J:so=\EG6:ue@:up=^K:us@:tc=adm+sgr:
