            <a class="tlp-tab" href="{{ emailgateway_url }}">{{ emailgateway }}</a>
            <a class="tlp-tab" href="{{ natures_url }}">{{ natures }}</a>
            <a class="tlp-tab" href="{{ report_config_url }}">{{ report_config_panel }}</a>
            <a class="tlp-tab tlp-tab-active" href="{{ artifacts_deletion_url }}">{{# dgettext }} tuleap-tracker | Artifacts deletion {{/ dgettext }}</a>
