<div class="tracker-form-element-artifactlink-trackerpanel">
    <h2 class="tracker-form-element-artifactlink-nature_{{ nature }}">{{ nature_label }}</h2>
    <table class="tracker_report_table table table-striped table-bordered">
        <thead>
            <th>{{ id_label }}</th>
            <th>{{ project_label }}</th>
            <th>{{ tracker_label }}</th>
            <th>{{ summary_label }}</th>
            <th>{{ status_label }}</th>
            <th>{{ last_update_label }}</th>
            <th>{{ submitted_by_label }}</th>
            <th>{{ assigned_to_label }}</th>
        </thead>
        <tbody>
        {{# artifact_links }}
            <tr>
                <td>{{{ direct_link_to_artifact }}}</td>
                <td>{{ project_public_name }}</td>
                <td>{{ tracker_name }}</td>
                <td>{{ artifact_title }}</td>
                <td>{{ artifact_status }}</td>
                <td>{{ artifact_last_update_date }}</td>
                <td>{{{ artifact_submitter }}}</td>
                <td>
                {{{ artifact_assignees }}}
                </td>
            </tr>
        {{/ artifact_links }}
        </tbody>
    </table>
</div>
